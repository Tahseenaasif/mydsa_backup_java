package bancktracking;

public class N_Queen {
	private static void queen(int[][] board, int row) {
	    int n=board.length;
		if(row==n) {
			for(int i=0;i<n;i++){
				for(int j=0;j<n;j++){
					System.out.print(board[i][j]+" ");
				}
			}
			System.out.println();
		}
		for(int col=0;col<n;col++){
			board[row][col]=1;
			queen(board,row+1);
			board[row][col]=0;
		}
		
		
	}

	public static void main(String[] args) {
	int n=4;
	int board[][]=new int[n][n];
	  queen(board,0);

	}

	
}
