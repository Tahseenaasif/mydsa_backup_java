package lecture1;

public class sub_string {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String str="pqrs";
		int n=str.length();
		for(int i=0;i<n;i++){
			for(int j=i;j<n;j++){
				for(int k=0;k<j;k++) {
				System.out.print(str.charAt(k));
			}
			System.out.println(" ");
		}

	}
	}
}

