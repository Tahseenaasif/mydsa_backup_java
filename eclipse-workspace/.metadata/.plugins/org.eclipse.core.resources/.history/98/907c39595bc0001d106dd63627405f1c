package stack;

import linked_list.Node;

public class using_linked_list {
     private stack_node<Integer> head;
    // private int  size;
  using_linked_list(){
	  head=null;
	  //size=0;
  }
  
  public void push(int element) {
	  stack_node<Integer> newnode=new  stack_node<Integer>(element);;
	  if(head==null) {
		  head=newnode;
	  }else {
		  newnode.next=head;
		  head=newnode;
	  }
  }
	  public int pop(){
		  int temp=head.data;
		  head=head.next;
		  return temp;
	  }
	  public int top() {
		  return head.data;
	  }
  }

